{
  "name": "@repo/storage",
  "private": true,
  "type": "module",
  "exports": {
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts",
    "./keys": {
      "types": "./keys.ts",
      "import": "./keys.ts"
    }
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "knip": "knip --fix --reporter json",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/lib-storage": "^3.850.0",
    "@aws-sdk/s3-request-presigner": "^3.850.0",
    "@repo/database": "workspace:*",
    "@repo/observability": "workspace:*",
    "@t3-oss/env-nextjs": "catalog:",
    "@vercel/blob": "^1.1.1",
    "chalk": "^5.4.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "react": "catalog:"
  }
}
