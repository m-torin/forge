{
  "name": "@repo/ai",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/shared.ts",
      "default": "./src/shared.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "default": "./src/server.ts"
    },
    "./client/next": {
      "types": "./src/client-next.ts",
      "default": "./src/client-next.ts"
    },
    "./server/next": {
      "types": "./src/server-next.ts",
      "default": "./src/server-next.ts"
    },
    "./server/edge": {
      "types": "./src/server-edge.ts",
      "default": "./src/server-edge.ts"
    },
    "./shared": {
      "types": "./src/shared.ts",
      "default": "./src/shared.ts"
    },
    "./server/rag": {
      "types": "./src/server/rag/index.ts",
      "default": "./src/server/rag/index.ts"
    },
    "./server/mcp": {
      "types": "./src/server/mcp/index.ts",
      "default": "./src/server/mcp/index.ts"
    },
    "./server/agents": {
      "types": "./src/server/agents/index.ts",
      "default": "./src/server/agents/index.ts"
    },
    "./models": {
      "types": "./src/shared/models/index.ts",
      "default": "./src/shared/models/index.ts"
    },
    "./rsc": {
      "types": "./src/rsc/index.ts",
      "default": "./src/rsc/index.ts"
    }
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "knip": "knip --fix --reporter json",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:integration": "INTEGRATION_TEST=true vitest run --config vitest.integration.config.ts",
    "test:integration:all": "INTEGRATION_TEST=true vitest run --grep=\"upgraded|Integration\"",
    "test:integration:mock": "vitest run --config vitest.integration.config.ts",
    "test:mock:all": "vitest run --grep=\"upgraded|Mock\"",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "catalog:",
    "@ai-sdk/deepinfra": "catalog:",
    "@ai-sdk/google": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@ai-sdk/openai-compatible": "catalog:",
    "@ai-sdk/perplexity": "catalog:",
    "@ai-sdk/provider": "catalog:",
    "@ai-sdk/provider-utils": "catalog:",
    "@ai-sdk/react": "catalog:",
    "@ai-sdk/rsc": "catalog:",
    "@ai-sdk/xai": "catalog:",
    "@anthropic-ai/sdk": "^0.57.0",
    "@mantine/core": "catalog:",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@repo/database": "workspace:*",
    "@repo/mcp-utils": "workspace:*",
    "@repo/observability": "workspace:*",
    "@t3-oss/env-nextjs": "catalog:",
    "@tabler/icons-react": "catalog:",
    "@upstash/vector": "^1.2.2",
    "ai": "catalog:",
    "exa-js": "^1.8.23",
    "next": "catalog:",
    "openai": "^5.10.2",
    "react": "catalog:",
    "react-markdown": "^10.1.0",
    "resumable-stream": "^2.2.1",
    "server-only": "^0.0.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@vitest/ui": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  }
