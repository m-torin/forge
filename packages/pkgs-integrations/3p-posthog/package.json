{
  "name": "@repo/3p-posthog",
  "version": "0.1.0",
  "description": "PostHog analytics integration with feature flags and experiments",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts",
    "./server/edge": "./src/server-edge.ts",
    "./config": "./src/config.ts",
    "./types": "./src/types.ts",
    "./operations/events": "./src/operations/events.ts",
    "./operations/feature-flags": "./src/operations/feature-flags.ts",
    "./operations/experiments": "./src/operations/experiments.ts",
