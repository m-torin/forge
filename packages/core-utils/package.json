{
  "name": "@repo/core-utils",
  "version": "0.1.0",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./shared/*": {
      "types": "./src/shared/*.ts",
      "import": "./src/shared/*.ts"
    },
    "./server/*": {
      "node": {
        "types": "./src/server/*.ts",
        "import": "./src/server/*.ts"
      },
      "default": "./src/server/browser-stub.ts"
    }
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "acorn": "^8.14.0",
    "glob": "^11.0.0",
    "lru-cache": "catalog:",
    "node-abort-controller": "^3.1.1",
    "p-limit": "^6.1.0",
    "p-queue": "^8.0.1",
    "picomatch": "^4.0.2",
    "shell-quote": "^1.8.1",
    "ts-morph": "^24.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/acorn": "^4.0.6",
    "@types/madge": "^5.0.3",
    "@types/node": "catalog:",
    "@types/picomatch": "^4.0.2",
    "@types/shell-quote": "^1.7.5",
    "madge": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22"
  }
}
