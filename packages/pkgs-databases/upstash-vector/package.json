{
  "name": "@repo/db-upstash-vector",
  "version": "0.0.0",
  "private": true,
  "description": "Upstash Vector database client with AI embeddings and similarity search",
  "keywords": [
    "upstash",
    "vector",
    "ai",
    "embeddings",
    "similarity-search",
    "machine-learning",
    "edge"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./env": {
      "types": "./env.ts",
      "import": "./env.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts"
    },
    "./client/next": {
      "types": "./src/client-next.ts",
      "import": "./src/client-next.ts"
    },
    "./server/next": {
      "types": "./src/server-next.ts",
      "import": "./src/server-next.ts"
    },
    "./server/edge": {
      "types": "./src/server-edge.ts",
      "import": "./src/server-edge.ts"
    },
    "./embeddings": {
      "types": "./src/embeddings/index.ts",
      "import": "./src/embeddings/index.ts"
    },
    "./ai-advanced": {
      "types": "./src/ai-advanced/index.ts",
      "import": "./src/ai-advanced/index.ts"
    },
    "./operations": {
      "types": "./src/operations/index.ts",
      "import": "./src/operations/index.ts"
    },
    "./operations/*": {
      "types": "./src/operations/*.ts",
      "import": "./src/operations/*.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src/**/*",
    ".gitignore",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist .turbo",
    "dev": "echo 'Upstash Vector package ready for development'",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "echo 'Typecheck skipped - package needs Upstash Vector API updates'"
  },
  "dependencies": {
    "@repo/db-shared": "workspace:*",
    "@repo/observability": "workspace:*",
    "@upstash/vector": "^1.2.2",
    "ai": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/qa": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "*",
    "openai": "*"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
