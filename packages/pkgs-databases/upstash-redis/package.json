{
  "name": "@repo/db-upstash-redis",
  "version": "0.0.0",
  "private": true,
  "description": "Upstash Redis client with rate limiting and edge runtime support",
  "keywords": [
    "upstash",
    "redis",
    "cache",
    "rate-limiting",
    "edge",
    "serverless"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./env": {
      "types": "./env.ts",
      "import": "./env.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts"
    },
    "./client/next": {
      "types": "./src/client-next.ts",
      "import": "./src/client-next.ts"
    },
    "./server/next": {
      "types": "./src/server-next.ts",
      "import": "./src/server-next.ts"
    },
    "./server/edge": {
      "types": "./src/server-edge.ts",
      "import": "./src/server-edge.ts"
    },
    "./ratelimit": {
      "types": "./src/ratelimit/index.ts",
      "import": "./src/ratelimit/index.ts"
    },
    "./operations": {
      "types": "./src/operations/index.ts",
      "import": "./src/operations/index.ts"
    },
    "./operations/*": {
      "types": "./src/operations/*.ts",
      "import": "./src/operations/*.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src/**/*",
    ".gitignore",
    "README.md"
  ],
  "scripts": {
    "build": "echo 'No build needed - consumed from source'",
    "clean": "rm -rf dist .turbo",
    "dev": "echo 'Upstash Redis package ready for development'",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "echo 'Typecheck skipped - package needs Redis type refactoring'"
  },
  "dependencies": {
    "@repo/db-shared": "workspace:*",
    "@repo/observability": "workspace:*",
    "@upstash/ratelimit": "^2.0.4",
    "@upstash/redis": "^1.35.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/qa": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
