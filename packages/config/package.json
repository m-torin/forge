{
  "name": "@repo/config",
  "private": true,
  "type": "module",
  "exports": {
    "./typescript": "./src/typescript/",
    "./typescript/*": "./src/typescript/*",
    "./next": "./src/next/index.ts",
    "./next/keys": "./keys.ts",
    "./prettier": "./src/prettier/index.mjs",
    "./knip": "./src/knip/index.json",
    "./knip/*": "./src/knip/*",
    "./keys": {
      "types": "./keys.ts",
      "import": "./keys.ts"
    }
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "knip": "knip --fix --reporter json",
    "prettier": "turbo run prettier:apps prettier:labs prettier:packages prettier:monorepo --filter=@repo/config --continue",
    "prettier:apps": "prettier --write --log-level error '../../apps/**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}' --config ./src/prettier/index.mjs --ignore-path ./.prettierignore",
    "prettier:labs": "prettier --write --log-level error '../../labs/**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}' --config ./src/prettier/index.mjs --ignore-path ./.prettierignore",
    "prettier:monorepo": "prettier --write --log-level error '../../*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}' '../../scripts/**/*' --config ./src/prettier/index.mjs --ignore-path ./.prettierignore",
    "prettier:packages": "prettier --write --log-level error '../../packages/**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}' --config ./src/prettier/index.mjs --ignore-path ./.prettierignore",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@next/bundle-analyzer": "15.4.4",
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.12.0",
    "@t3-oss/env-core": "^0.13.8",
    "@t3-oss/env-nextjs": "catalog:",
    "chalk": "^5.4.1",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-sh": "^0.18.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "zod": "catalog:"
  },
  "devDependencies": {
    "next": "catalog:",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "next": "catalog:"
  }
}
