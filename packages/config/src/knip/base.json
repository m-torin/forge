{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/index.ts",
    "src/index.tsx",
    "src/app/**/*.{ts,tsx}",
    "src/pages/**/*.{ts,tsx}",
    "app/**/*.{ts,tsx}",
    "pages/**/*.{ts,tsx}",
    "index.{ts,tsx}",
    "**/*.config.{js,ts,mjs}",
    "**/*.setup.{js,ts}",
    "**/*.test.{js,ts,tsx}",
    "**/*.spec.{js,ts,tsx}",
    "e2e/**/*.{js,ts,tsx}",
    "__tests__/**/*.{js,ts,tsx}",
    "scripts/**/*.{js,ts,mjs}",
    "vitest.config.{js,ts}",
    "playwright.config.{js,ts}",
    "eslint.config.{js,ts,mjs}",
    "tailwind.config.{js,ts}",
    "postcss.config.{js,mjs}",
    "next.config.{js,ts,mjs}",
    "tsconfig.json"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "app/**/*.{ts,tsx}",
    "pages/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "contexts/**/*.{ts,tsx}",
    "providers/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "**/*.config.{js,ts,mjs}",
    "**/*.test.{js,ts,tsx}",
    "**/*.spec.{js,ts,tsx}",
    "e2e/**/*.{js,ts,tsx}",
    "__tests__/**/*.{js,ts,tsx}",
    "scripts/**/*.{js,ts,mjs}"
  ],
  "ignore": [
    "node_modules/**",
    "dist/**",
    "build/**",
    ".next/**",
    "coverage/**",
    "*.d.ts",
    "**/*.d.ts",
    "pnpm-lock.yaml",
    "package-lock.json",
    "yarn.lock"
  ],
  "ignoreDependencies": ["@types/*"],
  "rules": {
    "files": "error",
    "dependencies": "error",
    "devDependencies": "error",
    "unlisted": "error",
    "exports": "error",
    "nsExports": "error",
    "classMembers": "error",
    "enumMembers": "error",
    "types": "error",
    "nsTypes": "error",
    "duplicates": "error"
  }
}
