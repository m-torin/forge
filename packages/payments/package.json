{
  "name": "@repo/payments",
  "private": true,
  "type": "module",
  "exports": {
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts",
    "./ai": "./src/ai.ts",
    "./keys": {
      "types": "./keys.ts",
      "import": "./keys.ts"
    }
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
