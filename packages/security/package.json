{
  "name": "@repo/security",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts",
    "./keys": {
      "types": "./keys.ts",
      "import": "./keys.ts"
    }
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "eslint . --fix --cache",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@arcjet/next": "1.0.0-beta.9",
    "@nosecone/next": "1.0.0-beta.9",
    "@t3-oss/env-nextjs": "catalog:",
    "@upstash/ratelimit": "^2.0.6",
    "server-only": "^0.0.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/db-upstash-redis": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "knip": "catalog:",
    "madge": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
