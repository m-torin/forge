{
  "name": "@repo/auth",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/types.ts",
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts",
    "./server/edge": "./src/server-edge.ts",
    "./actions": "./src/server-actions.ts",
    "./server-actions": "./src/server-actions.ts",
    "./shared": "./src/shared/auth.ts",
    "./shared-env": "./src/shared/auth.ts",
    "./types": "./src/types/index.ts",
    "./components": "./src/components/index.ts",
    "./testing": "./src/testing/index.ts",
    "./testing/mocks": "./src/testing/mocks/index.ts",
    "./config": "./src/shared/config.ts"
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "create-users-direct": "node --experimental-strip-types src/scripts/create-users-direct.ts",
    "create-users-direct:doppler": "doppler run -- node --experimental-strip-types src/scripts/create-users-direct.ts",
    "knip": "knip --fix --reporter json",
    "lint": "eslint . --fix",
    "setup-passwords": "node --experimental-strip-types src/scripts/setup-passwords.ts",
    "setup-passwords:doppler": "doppler run -- node --experimental-strip-types src/scripts/setup-passwords.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:ui": "vitest --ui --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@repo/analytics": "workspace:*",
    "@repo/database": "workspace:*",
    "@repo/email": "workspace:*",
    "@repo/observability": "workspace:*",
    "@repo/security": "workspace:*",
    "@t3-oss/env-nextjs": "catalog:",
    "@upstash/redis": "^1.25.1",
    "better-auth": "^1.3.3",
    "better-auth-harmony": "^1.2.5",
    "chalk": "^5.4.1",
    "next": "catalog:",
    "next-themes": "^0.4.6",
    "react": "catalog:",
    "server-only": "^0.0.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@vitest/ui": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  }
}
