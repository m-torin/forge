    "schema-dts": "^1.1.5",
    "server-only": "^0.0.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@types/lodash.merge": "^4.6.9",
    "@vitest/ui": "catalog:",
    "next": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
