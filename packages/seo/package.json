{
  "name": "@repo/seo",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.tsx",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.tsx",
    "./server/next": "./src/server-next.ts",
    "./structured-data": "./src/components/structured-data.tsx"
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "knip": "knip --fix --reporter json",
    "lint": "eslint . --fix",
