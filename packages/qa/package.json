{
  "name": "@repo/qa",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./vitest": {
      "types": "./dist/vitest/index.d.ts",
      "import": "./dist/vitest/index.mjs",
      "default": "./dist/vitest/index.mjs"
    },
    "./vitest/configs": {
      "types": "./dist/vitest/configs.d.ts",
      "import": "./dist/vitest/configs.mjs",
      "default": "./dist/vitest/configs.mjs"
    },
    "./vitest/mocks": {
      "types": "./dist/vitest/mocks/index.d.ts",
      "import": "./dist/vitest/mocks/index.mjs",
      "default": "./dist/vitest/mocks/index.mjs"
    },
    "./vitest/configs/*": {
      "types": "./dist/vitest/configs/*.d.ts",
      "import": "./dist/vitest/configs/*.mjs",
      "default": "./dist/vitest/configs/*.mjs"
    },
    "./vitest/setup/*": {
      "types": "./dist/vitest/setup/*.d.ts",
      "import": "./dist/vitest/setup/*.mjs",
      "default": "./dist/vitest/setup/*.mjs"
    },
    "./vitest/utils/*": {
      "types": "./dist/vitest/utils/*.d.ts",
      "import": "./dist/vitest/utils/*.mjs",
      "default": "./dist/vitest/utils/*.mjs"
    },
    "./vitest/mocks/*": {
      "types": "./dist/vitest/mocks/*.d.ts",
      "import": "./dist/vitest/mocks/*.mjs",
      "default": "./dist/vitest/mocks/*.mjs"
    },
    "./vitest/mocks/packages/*": {
      "types": "./dist/vitest/mocks/packages/*.d.ts",
      "import": "./dist/vitest/mocks/packages/*.mjs",
      "default": "./dist/vitest/mocks/packages/*.mjs"
    },
    "./vitest/mocks/providers/*": {
      "types": "./dist/vitest/mocks/providers/*.d.ts",
      "import": "./dist/vitest/mocks/providers/*.mjs",
      "default": "./dist/vitest/mocks/providers/*.mjs"
    },
    "./vitest/mocks/internal/*": {
      "types": "./dist/vitest/mocks/internal/*.d.ts",
      "import": "./dist/vitest/mocks/internal/*.mjs",
      "default": "./dist/vitest/mocks/internal/*.mjs"
    },
    "./playwright": {
      "types": "./dist/playwright/index.d.ts",
      "import": "./dist/playwright/index.mjs",
      "default": "./dist/playwright/index.mjs"
    },
    "./playwright/*": {
      "types": "./dist/playwright/*.d.ts",
      "import": "./dist/playwright/*.mjs",
      "default": "./dist/playwright/*.mjs"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.mjs",
      "default": "./dist/shared/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "qa-test-coverage": "./scripts/test-coverage.mjs"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "build": "tsup",
    "build:apps": "echo \"No build script for apps\"",
    "build:packages": "pnpm build",
    "build:watch": "tsup --watch",
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage": "node ./scripts/coverage.mjs",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "coverage:generate": "cd ../.. && pnpm turbo test:coverage --concurrency=10",
    "dev:apps": "echo \"No dev script for apps\"",
    "dev:packages": "echo \"No dev script for packages\"",
    "postinstall": "node scripts/install-playwright.mjs",
    "knip": "knip --fix --reporter json",
    "lint": "eslint . --fix",
    "lint:apps": "echo \"No lint script for apps\"",
    "lint:packages": "echo \"No lint script for packages\"",
    "playwright:install": "playwright install --with-deps chromium firefox webkit",
    "test": "vitest run",
    "test:apps": "echo \"No test script for apps\"",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:e2e": "playwright test",
    "test:packages": "echo \"No test script for packages\"",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "typecheck:apps": "echo \"No typecheck script for apps\"",
    "typecheck:packages": "echo \"No typecheck script for packages\""
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@faker-js/faker": "^9.9.0",
    "@mantine/core": "catalog:",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/browser": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "3.2.4",
    "columnify": "^1.6.0",
    "expect-type": "^1.2.2",
    "jsdom": "catalog:",
    "playwright": "^1.54.1",
    "react": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "webdriverio": "^9.18.4"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "tsup": "^8.0.0"
  }
}
