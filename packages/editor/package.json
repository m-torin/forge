{
  "name": "@repo/editor",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts",
    "./server/edge": "./src/server-edge.ts",
    "./components": "./src/components/index.ts",
    "./components/embeddable": "./src/components/EmbeddableNotionEditor/index.ts",
    "./embeddable": {
      "browser": "./dist/standalone/embeddable-standalone.global.js",
      "types": "./dist/standalone/embeddable-standalone.d.ts"
    },
    "./react": {
      "types": "./dist/react/react-bundle.d.ts",
      "import": "./dist/react/react-bundle.mjs",
      "require": "./dist/react/react-bundle.cjs"
    },
    "./nextjs": {
      "types": "./dist/nextjs/nextjs-bundle.d.ts",
      "import": "./dist/nextjs/nextjs-bundle.mjs",
      "require": "./dist/nextjs/nextjs-bundle.cjs"
    },
    "./hooks": "./src/hooks/index.ts",
    "./types": "./src/types/index.ts",
    "./providers": "./src/providers/index.ts",
    "./utils": "./src/utils/index.ts"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/standalone/embeddable-standalone.global.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "examples"
  ],
  "scripts": {
    "build": "tsup",
    "build:clean": "rm -rf dist && pnpm build",
    "build:dev": "tsup --watch",
    "build:test": "pnpm build && pnpm test:build",
    "build:verbose": "tsup --verbose",
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "eslint . --fix --cache",
    "prepublishOnly": "pnpm build:clean && pnpm test:build",
    "test": "vitest run",
    "test:build": "vitest run --config vitest.build.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:ui": "vitest --ui --coverage --no-open",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.0",
    "@mantine/core": "catalog:",
    "@mantine/form": "catalog:",
    "@mantine/hooks": "catalog:",
    "@mantine/modals": "catalog:",
    "@mantine/notifications": "catalog:",
    "@tabler/icons-react": "catalog:",
    "@tiptap/core": "^3.0.0",
    "@tiptap/extension-code-block-lowlight": "^3.0.0",
    "@tiptap/extension-collaboration": "^3.0.0",
    "@tiptap/extension-collaboration-caret": "^3.0.0",
    "@tiptap/extension-color": "^3.0.0",
    "@tiptap/extension-drag-handle": "^3.0.7",
    "@tiptap/extension-emoji": "^3.0.0",
    "@tiptap/extension-file-handler": "^3.0.7",
    "@tiptap/extension-highlight": "^3.0.0",
    "@tiptap/extension-horizontal-rule": "^3.0.0",
    "@tiptap/extension-link": "^3.0.0",
    "@tiptap/extension-list": "^3.0.0",
    "@tiptap/extension-mention": "^3.0.0",
    "@tiptap/extension-placeholder": "^3.0.0",
    "@tiptap/extension-table": "^3.0.0",
    "@tiptap/extension-table-cell": "^3.0.0",
    "@tiptap/extension-table-header": "^3.0.0",
    "@tiptap/extension-table-row": "^3.0.0",
    "@tiptap/extension-task-item": "^3.0.0",
    "@tiptap/extension-task-list": "^3.0.0",
    "@tiptap/extension-text-align": "^3.0.0",
    "@tiptap/extension-text-style": "^3.0.0",
    "@tiptap/extension-underline": "^3.0.0",
    "@tiptap/extension-unique-id": "^3.0.0",
    "@tiptap/extensions": "^3.0.0",
    "@tiptap/pm": "^3.0.0",
    "@tiptap/react": "^3.0.0",
    "@tiptap/starter-kit": "^3.0.0",
    "@tiptap/suggestion": "^3.0.0",
    "clsx": "catalog:",
    "dompurify": "^3.1.7",
    "file-type": "^19.5.0",
    "lowlight": "^3.1.0",
    "nanoid": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "sanitize-filename": "^1.6.3",
    "validator": "^13.12.0",
    "y-indexeddb": "^9.0.12",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.18",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/observability": "workspace:*",
    "@repo/qa": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/validator": "^13.12.2",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "knip": "catalog:",
    "madge": "catalog:",
    "storybook": "^9.0.18",
    "tsup": "^8.0.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
