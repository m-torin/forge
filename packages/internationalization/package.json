{
  "name": "@repo/internationalization",
  "private": true,
  "type": "module",
  "exports": {
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./client/next": "./src/client-next.ts",
    "./server/next": "./src/server-next.ts"
  },
  "scripts": {
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "eslint . --fix --cache",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:ui": "vitest --ui --no-open",
    "test:watch": "vitest",
    "translate": "npx -y languine@latest translate",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@repo/uix-system": "workspace:*",
    "negotiator": "^1.0.0",
    "next-international": "^1.3.1",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/observability": "workspace:*",
    "@repo/qa": "workspace:*",
    "@testing-library/react": "catalog:",
    "@types/negotiator": "^0.6.4",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "knip": "catalog:",
    "madge": "catalog:",
    "next": "catalog:",
    "react": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  }
}
