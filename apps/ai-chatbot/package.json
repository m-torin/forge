  "name": "ai-chatbot",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:check": "drizzle-kit check",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "pnpm exec node --experimental-strip-types lib/db/migrate.ts",
    "db:migrate:doppler": "pnpm exec doppler run -- pnpm run db:migrate",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:up": "drizzle-kit up",
    "dev": "next dev --turbo --port 3100",
    "format": "prettier --write .",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "eslint . --fix --cache",
    "start": "next start --port 3100",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "export PLAYWRIGHT=True && pnpm exec playwright test",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
