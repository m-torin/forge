{
  "name": "webapp",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:local": "next build",
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "dev": "next dev -p 3200",
    "doppler:open": "doppler open",
    "doppler:pull": "doppler secrets download --no-file --format env > .env.local",
    "doppler:secrets": "doppler secrets",
    "doppler:secrets:download": "doppler secrets download --no-file --format env",
    "doppler:secrets:upload": "doppler secrets upload .env.local",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "eslint . --fix --cache",
    "start": "next start",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@hugeicons/core-free-icons": "^1.0.16",
    "@hugeicons/react": "^1.0.5",
    "@repo/db-prisma": "workspace:*",
    "@repo/observability": "workspace:*",
    "@sentry/nextjs": "catalog:",
    "@t3-oss/env-nextjs": "catalog:",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "clsx": "catalog:",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "8.6.0",
    "next": "catalog:",
    "rc-slider": "^11.1.8",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hot-toast": "^2.5.2",
    "react-swipeable": "^7.0.2",
    "react-use": "^17.6.0",
    "sass": "^1.89.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@tailwindcss/postcss": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "concurrently": "^9.2.0",
    "eslint": "catalog:",
    "knip": "catalog:",
    "madge": "catalog:",
    "postcss": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
