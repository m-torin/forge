{
  "name": "docs",
  "private": true,
  "scripts": {
    "build": "echo 'Mintlify docs build'",
    "build:local": "echo 'Mintlify docs build'",
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "dev": "mintlify dev --port 3800",
    "doppler:open": "doppler open",
    "doppler:pull": "doppler secrets download --no-file --format env > .env.local",
    "doppler:secrets": "doppler secrets",
    "doppler:secrets:download": "doppler secrets download --no-file --format env",
    "doppler:secrets:upload": "doppler secrets upload .env.local",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "mintlify broken-links",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@repo/observability": "workspace:*",
    "mintlify": "^4.2.33"
  },
  "devDependencies": {
    "@repo/qa": "workspace:*",
    "@vitest/coverage-v8": "catalog:",
    "knip": "catalog:",
    "madge": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  }
}
