{
  "name": "storybook",
  "private": true,
  "scripts": {
    "build": "storybook build --quiet --loglevel warn",
    "build:doppler": "doppler run -- storybook build --quiet --loglevel warn",
    "chromatic": "chromatic --exit-zero-on-changes",
    "chromatic:doppler": "doppler run -- chromatic --exit-zero-on-changes",
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "coverage:collect": "vitest run --coverage --reporter=json",
    "dev": "storybook dev -p 3700 --quiet --loglevel error --no-open",
    "doppler:open": "doppler open",
    "doppler:pull": "doppler secrets download --no-file --format env > .env.local",
    "doppler:secrets": "doppler secrets",
    "doppler:secrets:download": "doppler secrets download --no-file --format env",
    "doppler:secrets:upload": "doppler secrets upload .env.local",
    "knip": "knip",
    "lint": "eslint . --fix",
    "start": "pnpm exec serve storybook-static -l 3700",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:json": "vitest run --coverage --reporter=json",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@mantine/code-highlight": "catalog:",
    "@mantine/core": "catalog:",
    "@mantine/hooks": "catalog:",
    "@repo/design-system": "workspace:*",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@storybook/addon-docs": "9.0.17",
    "@storybook/addon-onboarding": "9.0.17",
    "@storybook/addon-themes": "9.0.17",
    "@storybook/nextjs": "9.0.17",
    "@t3-oss/env-nextjs": "catalog:",
    "@tailwindcss/postcss": "catalog:",
    "buffer": "6.0.3",
    "chromatic": "13.1.2",
    "crypto-browserify": "3.12.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.0.17",
    "eslint-plugin-testing-library": "^7.6.0",
    "eslint-plugin-vitest": "^0.5.4",
    "events": "3.3.0",
    "path-browserify": "1.0.1",
    "postcss": "catalog:",
    "process": "0.11.10",
    "serve": "14.2.4",
    "storybook": "9.0.17",
    "stream-browserify": "3.0.0",
    "string_decoder": "1.3.0",
    "tailwind-preset-mantine": "catalog:",
    "tailwindcss": "catalog:",
    "util": "0.12.5",
    "zod": "catalog:"
  }
}
