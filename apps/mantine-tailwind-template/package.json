{
  "name": "next-app-mantine-tailwind-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:local": "next build",
    "circular": "madge --circular --extensions ts,tsx,js,jsx .",
    "dev": "next dev -p 3900",
    "doppler:open": "doppler open",
    "doppler:pull": "doppler secrets download --no-file --format env > .env.local",
    "doppler:secrets": "doppler secrets",
    "doppler:secrets:download": "doppler secrets download --no-file --format env",
    "doppler:secrets:upload": "doppler secrets upload .env.local",
    "knip": "knip --reporter json --exclude unlisted,unresolved,exports,dependencies,files,binaries,types,duplicates",
    "lint": "eslint . --fix --cache",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:ui": "vitest --ui --no-open",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "overrides": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:"
  },
  "dependencies": {
    "@mantine/core": "catalog:",
    "@mantine/form": "catalog:",
    "@mantine/hooks": "catalog:",
    "@repo/analytics": "workspace:*",
    "@repo/config": "workspace:*",
    "@repo/feature-flags": "workspace:*",
    "@repo/internationalization": "workspace:*",
    "@repo/observability": "workspace:*",
    "@t3-oss/env-nextjs": "catalog:",
    "@tabler/icons-react": "catalog:",
    "better-auth": "^1.3.4",
    "clsx": "catalog:",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@playwright/test": "catalog:",
    "@repo/eslint-config": "workspace:*",
    "@repo/qa": "workspace:*",
    "@tailwindcss/postcss": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "knip": "catalog:",
    "madge": "catalog:",
    "postcss": "catalog:",
    "tailwind-preset-mantine": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "catalog:"
  }
}
