{
  "name": "@labs/flows",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "circular": "madge --circular --extensions ts,tsx,js,jsx --exclude 'src/lib/prisma/generated' .",
    "dev": "next dev --port 3000",
    "knip": "knip --fix --reporter json",
    "lint": "eslint . --fix",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "copy-monaco-workers": "node scripts/copy-workers.mjs",
    "prisma": "prisma",
    "prisma:db:pull": "prisma db pull",
    "prisma:db:push": "prisma db push",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma format && prisma generate --schema=./src/lib/prisma/schema.prisma && node scripts/add-ts-nocheck.mjs",
    "prisma:introspect": "prisma introspect",
    "prisma:migrate:dev": "prisma validate && prisma migrate dev --schema=./src/lib/prisma/schema.prisma",
    "prisma:migrate:resolve": "prisma migrate resolve",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:push": "prisma format && prisma db push && pnpm build",
    "prisma:studio": "prisma studio --schema=./src/lib/prisma/schema.prisma",
    "prisma:validate": "prisma validate"
  },
  "prisma": {
    "schema": "./src/lib/prisma/schema.prisma"
  },
